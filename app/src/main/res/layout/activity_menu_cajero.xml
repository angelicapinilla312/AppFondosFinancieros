<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuCajero">

    <TextView
        android:id="@+id/textView29"
        android:layout_width="173dp"
        android:layout_height="26dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/autour_one"
        android:text="Participant"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnParticipantes" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="173dp"
        android:layout_height="51dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/autour_one"
        android:text="Terms, conditions and Rules"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnReglas" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="171dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/autour_one"
        android:text="Tracing"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSeguimiento" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="173dp"
        android:layout_height="26dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/autour_one"
        android:text="Loans"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPrestamos" />

    <TextView
        android:id="@+id/txtVwBalance"
        android:layout_width="293dp"
        android:layout_height="75dp"
        android:layout_marginTop="11dp"
        android:background="#C888D3"
        android:fontFamily="@font/autour_one"
        android:gravity="center"
        android:orientation="horizontal"
        android:outlineProvider="bounds"
        android:text="@string/msgBalance"
        android:textAlignment="center"
        android:textDirection="locale"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtVwTotalFondo" />

    <TextView
        android:id="@+id/txtVwAdminName"
        android:layout_width="189dp"
        android:layout_height="39dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="79dp"
        android:fontFamily="@font/autour_one"
        android:text="Admin Name"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/txtSaludo" />

    <TextView
        android:id="@+id/txtVwTotalFondo"
        android:layout_width="242dp"
        android:layout_height="39dp"
        android:layout_marginTop="48dp"
        android:fontFamily="@font/autour_one"
        android:text="Fund balance"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.349"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/txtSaludo"
        android:layout_width="182dp"
        android:layout_height="37dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="52dp"
        android:fontFamily="@font/autour_one"
        android:text="Hello"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="134dp"
        android:layout_height="89dp"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_user" />

    <ImageView
        android:id="@+id/btnParticipantes"
        android:layout_width="119dp"
        android:layout_height="111dp"
        android:layout_marginTop="32dp"
        android:contentDescription="partiTomenu"
        app:layout_constraintEnd_toStartOf="@+id/btnPrestamos"
        app:layout_constraintHorizontal_bias="0.41"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtVwBalance"
        app:srcCompat="@drawable/ic_parti" />

    <ImageView
        android:id="@+id/btnPrestamos"
        android:layout_width="119dp"
        android:layout_height="111dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="44dp"
        android:contentDescription="prestamoToMenu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtVwBalance"
        app:srcCompat="@drawable/prestamo" />

    <ImageView
        android:id="@+id/btnReglas"
        android:layout_width="119dp"
        android:layout_height="111dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="20dp"
        android:contentDescription="reglasToHome"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView29"
        app:srcCompat="@drawable/lista" />

    <ImageView
        android:id="@+id/btnSeguimiento"
        android:layout_width="119dp"
        android:layout_height="111dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="44dp"
        android:contentDescription="historicoToMenu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/btnReglas"
        app:layout_constraintTop_toBottomOf="@+id/textView32"
        app:srcCompat="@drawable/seguimiento" />

    <Button
        android:id="@+id/btnBackToHome"
        android:layout_width="206dp"
        android:layout_height="48dp"
        android:layout_marginBottom="44dp"
        android:backgroundTint="#0B5C64"
        android:fontFamily="@font/autour_one"
        android:text="Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView35"
        app:layout_constraintVertical_bias="0.782" />

</androidx.constraintlayout.widget.ConstraintLayout>