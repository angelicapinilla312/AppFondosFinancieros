<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ParticipantesCajero"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="-65dp">

    <TextView
        android:id="@+id/txtParticipantes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="165dp"
        android:fontFamily="@font/autour_one"
        android:text="@string/participants"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtListaParticipantes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginTop="17dp"
        android:layout_marginEnd="133dp"
        android:fontFamily="@font/autour_one"
        android:text="@string/participants_list"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="354dp"
        android:layout_height="367dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="29dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtParticipantes">

        <TextView
            android:id="@+id/textView33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/autour_one"
            android:text="@string/id_card"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/edtxDocument"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:ems="30"
            android:fontFamily="@font/autour_one"
            android:hint="@string/write_id_card"
            android:importantForAutofill="no"
            android:inputType="textPersonName"
            android:textColorHint="#9E9E9E" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/autour_one"
            android:text="@string/name"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/edtxName"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:ems="10"
            android:fontFamily="@font/autour_one"
            android:hint="@string/write_name"
            android:importantForAutofill="no"
            android:inputType="textPersonName"
            android:textColorHint="#9E9E9E" />

        <TextView
            android:id="@+id/textView36"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/autour_one"
            android:text="@string/lastname"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/edtxLastName"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:ems="10"
            android:fontFamily="@font/autour_one"
            android:hint="@string/write_lastname"
            android:importantForAutofill="no"
            android:inputType="textPersonName"
            android:textColorHint="#9E9E9E" />

        <TextView
            android:id="@+id/textView37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/autour_one"
            android:text="@string/phonenumber"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/edtxPhoneNumber"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:ems="10"
            android:fontFamily="@font/autour_one"
            android:hint="@string/write_phonenumber"
            android:importantForAutofill="no"
            android:inputType="textPersonName|textCapSentences"
            android:textColorHint="#9E9E9E" />

        <TextView
            android:id="@+id/textView38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/autour_one"
            android:text="@string/address"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/edtxAddress"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:ems="10"
            android:fontFamily="@font/autour_one"
            android:hint="@string/write_address"
            android:importantForAutofill="no"
            android:inputType="textPersonName"
            android:textColorHint="#9E9E9E" />

    </LinearLayout>

    <ListView
        android:id="@+id/listViewParticipants"
        android:layout_width="410dp"
        android:layout_height="176dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtListaParticipantes" />

    <Button
        android:id="@+id/btnBackParticipantHome"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:backgroundTint="#0B5C64"
        android:fontFamily="@font/autour_one"
        android:text="@string/back"
        android:textColor="#FAFAFA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listViewParticipants"
        app:layout_constraintVertical_bias="0.156" />

</androidx.constraintlayout.widget.ConstraintLayout>